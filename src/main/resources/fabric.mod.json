{
  "schemaVersion": 1,
  "id": "microchip",
  "version": "1.2.0",

  "name": "Microchip",
  "description": "Allows players to keep track of their mobs across the world!",
  "authors": [
    "jumpcutfindo",
    "eggtitan"
  ],
  "contact": {
    "homepage": "https://github.com/jumpcutfindo/microchip",
    "sources": "https://github.com/jumpcutfindo/microchip"
  },

  "license": "CC0-1.0",
  "icon": "assets/microchip/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.jumpcutfindo.microchip.MicrochipMod",
      "com.jumpcutfindo.microchip.server.network.ServerNetworkReceiver",
      "com.jumpcutfindo.microchip.server.network.ServerNetworkSender"
    ],
    "client": [
      "com.jumpcutfindo.microchip.client.InputListener",
      "com.jumpcutfindo.microchip.client.network.ClientNetworkReceiver",
      "com.jumpcutfindo.microchip.client.network.ClientNetworkSender"
    ],
    "cardinal-components": [
      "com.jumpcutfindo.microchip.data.MicrochipComponents"
    ]
  },
  "mixins": [
  ],

  "custom": {
    "cardinal-components": [
      "microchip:microchips"
    ]
  },

  "depends": {
    "fabricloader": ">=0.16.10",
    "fabric-api": "*",
    "minecraft": "^1.21.2",
    "java": ">=21"
  },
  "suggests": {
  }
}
